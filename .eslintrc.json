{
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "plugin:jest/recommended"
    ],
    "plugins": ["prettier", "jest"],
    "rules": {
        "prettier/prettier": "error",
        "no-underscore-dangle": ["error", { "allow": ["_id"] }]
    },
    "env": {
        "node": true,
        "jest/globals": true
    }
}